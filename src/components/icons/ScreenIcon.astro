---
export interface Props {
	color?: string;
}

const { color = "currentColor" } = Astro.props;
---

<svg width="100%" height="100%" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
	<rect 
		fill="none" 
		stroke={color} 
		stroke-linecap="round" 
		stroke-linejoin="round" 
		stroke-width="2" 
		height="24" 
		rx="2" 
		ry="2" 
		width="30" 
		x="1" 
		y="1"
	/>
	<polygon 
		fill="none" 
		stroke={color} 
		stroke-linecap="round" 
		stroke-linejoin="round" 
		stroke-width="2" 
		points="21 31 11 31 12 25 20 25 21 31"
	/>
	<line 
		stroke={color} 
		stroke-linecap="round" 
		stroke-linejoin="round" 
		stroke-width="2" 
		x1="9" 
		x2="23" 
		y1="31" 
		y2="31"
	/>
	<line 
		stroke={color} 
		stroke-linecap="round" 
		stroke-linejoin="round" 
		stroke-width="2" 
		x1="1" 
		x2="31" 
		y1="21" 
		y2="21"
	/>
</svg>